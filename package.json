{
  "name": "to-do-app",
  "version": "1.0.0",
  "description": "Simple todo app with login and register page using express cookie sessions",
  "main": "app.js",
  "scripts": {
    "prestart": "npm install",
    "start": "node app",
    "dev": "nodemon app --ignore ./data-db",
    "dev:debug": "DEBUG=*,-nodemon*,-send* npm run dev",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "keywords": [
    "session",
    "express",
    "cookies",
    "login",
    "to-do-app"
  ],
  "author": "jalbertsr",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.2",
    "cookie-session": "^1.3.0",
    "dotenv": "^16.6.1",
    "express": "^4.15.4",
    "mysql2": "^3.6.0",
    "pug": "^2.0.0-rc.3",
    "strftime": "^0.10.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.2.2"
  }
}
